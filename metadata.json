{
      "name": "martezr-rdp",
      "version": "0.1.1",
      "author": "Martez Reed",
      "license": "Apache-2.0",
      "summary":     "Puppet module to enable/disable Remote Desktop and RDP Network Level Authentication",
      "source": "https://github.com/martezr/puppet-rdp",
      "project_page": "https://github.com/martezr/puppet-rdp",
      "issues_url": "https://github.com/martezr/puppet-rdp/issues",
      "tags": ["rdp"],
      "operatingsystem_support": [
       {
         "operatingsystem": "windows",
         "operatingsystemrelease": [
          "Server 2008",
          "Server 2008 R2", 
          "Server 2012", 
          "Server 2012 R2", 
          "7",
          "8"
         ]
        }
      ], 
      "requirements": [
        {
          "name": "pe",
          "version_requirement": ">= 3.3.0 < 2015.3.0"
        },
        {
          "name": "puppet",
          "version_requirement": ">= 3.0.0 < 5.0.0"
        }
      ],
      "dependencies": [
        { "name": "puppetlabs/registry", "version_requirement": ">= 0.1.0 <5.0.0" },
        { "name": "puppetlabs/stdlib", "version_requirement": ">= 0.1.0 <5.0.0" },
        { "name": "puppet/windows_firewall", "version_requirement": ">= 0.1.0 <5.0.0" }
      ]
    }
